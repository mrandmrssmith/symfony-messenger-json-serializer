{
    "name": "mrandmrssmith/symfony-messenger-json-serializer",
    "description": "Provide a JSON serializer for Symfony Messenger messages",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/messenger": ">=4.4",
        "symfony/serializer": ">=4.4",
        "symfony/property-access": ">=4.4"
    },
    "authors": [
        {
            "name": "Guillaume Vermeersch",
            "email": "guillaume.vermeersch@smithhotels.com"
        },
        {
            "name": "Jack Wilson",
            "email": "jack.wilson@smithhotels.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MrAndMrsSmith\\SymfonyMessengerJSONSerializer\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": ">=8.5",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^12.1",
        "vimeo/psalm": "^4.30||^5.22"
    }
}
